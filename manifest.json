{  
    "manifest_version": 2,  
    "name": "Desmos",  
    "description": "Extension for desmos.com",  
    "version": "1.0",
    "icons": {
        "16": "assets/favicon.png",
        "64": "assets/favicon.png",
        "128": "assets/favicon.png"
      },  
    "browser_action": {
     "popup": "popup.html",
     "default_popup": "popup.html"  
    },
    "commands": {
      "_execute_page_action": {
        "suggested_key": {
          "windows": "Ctrl+Shift+D",
          "mac":"Command+Shift+D"
        }
      }
}
}